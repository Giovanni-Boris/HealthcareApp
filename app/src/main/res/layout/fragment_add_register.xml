            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/actividad" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/floatingActionButton"
            android:layout_width="33dp"
            android:layout_height="29dp"
            android:layout_marginStart="80dp"
            android:layout_marginTop="24dp"
            android:clickable="true"
            app:layout_constraintStart_toEndOf="@+id/glucemia"
            app:layout_constraintTop_toBottomOf="@+id/editTextTime"
            app:srcCompat="@drawable/circle" />

        <TextView
            android:id="@+id/pesoCorporal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="20dp"
            android:text="Peso Corporal"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tensionArterial" />

        <EditText
            android:id="@+id/inputGlucemia"
            android:layout_width="56dp"
            android:layout_height="31dp"
            android:layout_marginStart="22dp"
            android:layout_marginTop="20dp"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:hint="-"
            android:inputType="numberDecimal"
            android:textColor="#000000"
            android:textColorHint="#000000"
            app:layout_constraintStart_toEndOf="@+id/floatingActionButton"
            app:layout_constraintTop_toBottomOf="@+id/editTextTime" />

        <EditText
            android:id="@+id/inputInsulinaComida"
            android:layout_width="59dp"
            android:layout_height="25dp"
            android:layout_marginStart="21dp"
            android:layout_marginTop="15dp"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:hint="-"
            android:inputType="numberDecimal"
            android:textColor="#000000"
            android:textColorHint="#000000"
            app:layout_constraintStart_toEndOf="@+id/floatingActionButton3"
