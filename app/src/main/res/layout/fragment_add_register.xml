            android:ems="10"
            android:gravity="center"
            android:hint="-"
            android:inputType="numberDecimal"
            android:textColor="#000000"
            android:textColorHint="#000000"
            app:layout_constraintStart_toEndOf="@+id/floatingActionButton8"
